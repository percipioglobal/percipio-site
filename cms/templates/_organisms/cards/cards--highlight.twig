{%- minify -%}

    {% set featured = hasFeatured ?? null %}
    {% set cardsType = type ?? 'default' %}

    {%- switch cardsType -%}

        {%- case 'content' -%}
            {%- set cards = content.cards.all() -%}

        {% default %}
            {%- set cards = content -%}

    {%- endswitch -%}

    <div class="print:hidden md:grid grid-cols-6 gap-8 my-16">
        {%- for card in cards -%}
            {% switch card.type %}
                {% case "teamPage" %}
                    {%- include '_molecules/cards/card--team' ignore missing with {
                        content: card,
                    } -%}
                {% default %}
                    {%- include '_molecules/cards/card--highlight' ignore missing with {
                        content: card,
                    } -%}
            {% endswitch %}

        {%- endfor -%}

    </div>



{%- endminify -%}